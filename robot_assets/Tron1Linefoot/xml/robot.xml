<mujoco model="SF_TRON1">
    <compiler angle="radian" meshdir="../meshes/" />
    <size njmax="500" nconmax="100" />

    <default>
        <light castshadow="false" diffuse="1 1 1" />
        <camera fovy="60"/>
        <default class="visual">
            <geom contype="0" conaffinity="0" group="1" type="mesh" rgba="0.9 0.9 0.9 1.0"/>
        </default>
        <joint armature="0.01" damping="0.01"/>
        <geom contype="1" conaffinity="1" condim="3" friction="0.8 0.005 0.0001" rgba="0.5 0.6 0.7 0.0" margin="0.001" group="0" />
    </default>
    <visual>
        <rgba com="0.502 1.0 0 0.5" contactpoint="1.0 1.0 0.6 0.4" contactforce="0.2 0.98 0.2 0.8"/>
        <scale com="0.2" forcewidth="0.05" contactwidth="0.10" contactheight="0.04"/>
    </visual>

    <asset>
        <mesh name="base_Link" file="base_Link.STL" />
        <mesh name="abad_L_Link" file="abad_L_Link.STL" />
        <mesh name="hip_L_Link" file="hip_L_Link.STL" />
        <mesh name="knee_L_Link" file="knee_L_Link.STL" />
        <mesh name="ankle_L_Link" file="ankle_L_Link.STL" />
        <mesh name="abad_R_Link" file="abad_R_Link.STL" />
        <mesh name="hip_R_Link" file="hip_R_Link.STL" />
        <mesh name="knee_R_Link" file="knee_R_Link.STL" />
        <mesh name="ankle_R_Link" file="ankle_R_Link.STL" />
    </asset>

    <asset>
        <texture type='skybox' builtin='gradient' width='512' height='512' rgb1="1.0 1.0 1.0" rgb2="1.0 1.0 1.0"/>
        <texture name="texplane" type="2d" builtin="checker" rgb1="0.35 0.35 0.35" rgb2="0.55 0.55 0.55" width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>
        <material name="matplane" reflectance="0.0" texture="texplane" texrepeat="1 1" texuniform="true"/>
    </asset>
        <option timestep="0.001"/>

    <keyframe>
        <key name="0" qpos="0 0 0.55 1 0 0 0 0 0.8 -1.5 0.7 0 0.8 -1.5 0.7" />
    </keyframe>

    <!-- <equality>
        <weld name="fix_torso" body1="base_Link" relpose="0 0 -.55 1 0 0 0" solimp="0.9 0.95 0.001 0.5 2" solref="0.02 1"/>
    </equality> -->

    <worldbody>
        <geom name="floor" rgba="0.5 0.6 0.7 1" pos="0 0 0" size="0 0 0.25" type="plane" material="matplane" condim="3"/>
        <camera name="track" mode="trackcom" pos="0 -2.3 1.6" xyaxes="1 0 0 0 0.707 0.707"/>
        <light directional="true" diffuse=".8 .8 .8" pos="0 0 10" dir="0 0 -10"/>

        <body name="base_Link" pos="0 0 0">
            <inertial pos="0.0457123 0.000139854 0.0964" quat="0.972578 -0.00615364 -0.232461 0.00398682" mass="12" diaginertia="0.154712 0.11093 0.0846371" />
            <joint type="free" limited="false" damping="0.0000" stiffness="0.0000" armature="0"/>
            <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="base_Link" pos="0 0 0.2602"/>
            <geom name="base_collision" size="0.135 0.13 0.095" pos="0.03 0 0.1882" type="box" rgba="0 0 1 0" />

            <!-- <site name="imu" pos="0 0 0"/> -->
            <site name="imu" pos="0 0 0.2602"/>
            <site class="visual" name="com" pos="0 0 0" type="sphere" size="0.03" rgba="1 0 0 0.8" />

            <body name="abad_R_Link" pos="0.05556 -0.105 0">
                <inertial pos="-0.06977 -0.04479 0.00057" quat="0.393019 0.394141 0.579772 0.595024" mass="1.469" diaginertia="0.00252289 0.00208104 0.00139163" />
                <joint name="abad_R_Joint" pos="0 0 0" axis="1 0 0" limited="true" range="-1.39626 0.38397" />
                <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="abad_R_Link" />
                <geom name="abad_R_collision" size="0.05 0.025" pos="0.03 0 0" quat="0.500398 0.5 0.499602 0.5" type="cylinder" rgba="0 0 1 0" />
                
                <body name="hip_R_Link" pos="-0.077 -0.0205 0">
                    <inertial pos="0 0.0477 -0.0492" quat="0.4625   -0.0046    0.0743    0.8835" mass="2.3" diaginertia="0.0233264 0.0230229 0.00277548"/>
                    <joint name="hip_R_Joint" pos="0 0 0" axis="0 1 0" limited="true" range="-1.01229 1.39626" />
                    <geom class="visual" rgba="1 1 1 1" mesh="hip_R_Link" euler="0 -0.53 0"/>
                    <geom name="hip_R_collision" size="0.035 0.075" pos="-0.02 0.03 -0.172" type="cylinder" rgba="0 0 1 0" />
                    
                    <body name="knee_R_Link" pos="0 0.0205 -0.3">
                        <inertial pos="0 -0.0013 -0.225" mass="1.22" diaginertia="0.0144103 0.0143581 0.00061582" />
                        <joint name="knee_R_Joint" pos="0 0 0" axis="0 1 0" limited="true" range="-1.36136 0.872665" />
                        <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="knee_R_Link" euler="0 0.55 0"/>
                        <geom name="knee_R_collision" size="0.02 0.13" pos="0 0 -0.1431" type="cylinder" rgba="0 0 1 0" />
                        
                        <body name="ankle_R_Link" pos="0 0 -0.3">
                            <inertial pos="0.01074 0.00061 -0.03314" mass="0.62" diaginertia="0.0019759 0.00181179 0.000523633" euler="0 1.57 0"/>
                            <joint name="ankle_R_Joint" pos="0 0 0" axis="0 1 0" limited="true" range="-1.5708 1.15192" frictionloss="0.01" />
                            <geom class="visual" rgba="0.26667 0.26667 0.26667 1" mesh="ankle_R_Link" />
                            <geom name="ankle_R_collision" type="mesh" rgba="0 0 1 0" mesh="ankle_R_Link" />

                            <site name="heel_R_site" type="sphere" pos="-0.08 0 -0.05" size="0.03"/>
                            <site name="toe_R_site" type="sphere" pos="0.1 0 -0.05" size="0.03"/>
                        </body>
                    </body>
                </body>
            </body>
            
            <body name="abad_L_Link" pos="0.05556 0.105 0">
                <inertial pos="-0.06977 0.04479 0.00057" quat="0.595024 0.579772 0.394141 0.393019" mass="1.469" diaginertia="0.00252289 0.00208104 0.00139163" />
                <joint name="abad_L_Joint" pos="0 0 0" axis="1 0 0" limited="true" range="-0.38397 1.39626" />
                <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="abad_L_Link" />
                <geom name="abad_L_collision" size="0.05 0.025" pos="0.03 0 0" quat="0.500398 0.5 0.499602 0.5" type="cylinder" rgba="0 0 1 0" />
                
                <body name="hip_L_Link" pos="-0.077 0.0205 0">
                    <inertial pos="0 -0.0477 -0.0492" quat="0.8835    0.0743   -0.0046    0.4625" mass="2.3" diaginertia="0.0233264 0.0230229 0.00277548" />
                    <joint name="hip_L_Joint" pos="0 0 0" axis="0 1 0" limited="true" range="-1.01229 1.39626" />
                    <geom class="visual" rgba="1 1 1 1" mesh="hip_L_Link" euler="0 -0.53 0"/>
                    <geom name="hip_L_collision" size="0.035 0.075" pos="-0.02 -0.03 -0.172" type="cylinder" rgba="0 0 1 0" />
                    
                    <body name="knee_L_Link" pos="0 -0.0205 -0.3">
                        <inertial pos="0 0.0013 -0.225" mass="1.22" diaginertia="0.0144103 0.0143581 0.00061582" />
                        <joint name="knee_L_Joint" pos="0 0 0" axis="0 1 0" limited="true" range="-1.36136 0.872665" />
                        <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="knee_L_Link" euler="0 0.55 0"/>
                        <geom name="knee_L_collision" size="0.02 0.13" pos="0 0 -0.1431" type="cylinder" rgba="0 0 1 0" />
                        
                        <body name="ankle_L_Link" pos="0 0 -0.3">
                            <inertial pos="0.01074 -0.00061 -0.03314" mass="0.62" diaginertia="0.0019759 0.00181179 0.000523633" euler="0 1.57 0"/>
                            <joint name="ankle_L_Joint" pos="0 0 0" axis="0 1 0" limited="true" range="-1.5708 1.15192" frictionloss="0.01" />
                            <geom class="visual" rgba="0.26667 0.26667 0.26667 1" mesh="ankle_L_Link" />
                            <geom name="ankle_L_collision" type="mesh" rgba="0 0 1 0" mesh="ankle_L_Link" />

                            <site name="heel_L_site" type="sphere" pos="-0.08 0 -0.05" size="0.03"/>
                            <site name="toe_L_site" type="sphere" pos="0.1 0 -0.05" size="0.03"/>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <motor name="abad_R_Joint"   gear="1"  joint="abad_R_Joint"  ctrllimited='true' ctrlrange='-80 80' />
        <motor name="hip_R_Joint"    gear="1"  joint="hip_R_Joint"   ctrllimited='true' ctrlrange='-80 80' />
        <motor name="knee_R_Joint"   gear="1"  joint="knee_R_Joint"  ctrllimited='true' ctrlrange='-80 80'/>
        <motor name="ankle_R_Joint"  gear="1"  joint="ankle_R_Joint" ctrllimited='true' ctrlrange='-20 20'/>
        <motor name="abad_L_Joint"   gear="1"  joint="abad_L_Joint"  ctrllimited='true' ctrlrange='-80 80'/>
        <motor name="hip_L_Joint"    gear="1"  joint="hip_L_Joint"   ctrllimited='true' ctrlrange='-80 80'/>
        <motor name="knee_L_Joint"   gear="1"  joint="knee_L_Joint"  ctrllimited='true' ctrlrange='-80 80'/>
        <motor name="ankle_L_Joint"  gear="1"  joint="ankle_L_Joint" ctrllimited='true' ctrlrange='-20 20'/>
    </actuator>

    <sensor>
        <jointpos name="abad_R_Joint_pos"    joint="abad_R_Joint" noise="0.01"/>
        <jointpos name="hip_R_Joint_pos"     joint="hip_R_Joint" noise="0.01"/>
        <jointpos name="knee_R_Joint_pos"    joint="knee_R_Joint" noise="0.01"/>
        <jointpos name="ankle_R_Joint_pos"   joint="ankle_R_Joint" noise="0.01"/>
        <jointpos name="abad_L_Joint_pos"    joint="abad_L_Joint" noise="0.01"/>
        <jointpos name="hip_L_Joint_pos"     joint="hip_L_Joint" noise="0.01"/>
        <jointpos name="knee_L_Joint_pos"    joint="knee_L_Joint" noise="0.01"/>
        <jointpos name="ankle_L_Joint_pos"   joint="ankle_L_Joint" noise="0.01"/>

        <jointvel name="abad_R_Joint_vel"    joint="abad_R_Joint" noise="0.01"/>
        <jointvel name="hip_R_Joint_vel"     joint="hip_R_Joint" noise="0.01"/>
        <jointvel name="knee_R_Joint_vel"    joint="knee_R_Joint" noise="0.01"/>
        <jointvel name="ankle_R_Joint_vel"   joint="ankle_R_Joint" noise="0.01"/>
        <jointvel name="abad_L_Joint_vel"    joint="abad_L_Joint" noise="0.01"/>
        <jointvel name="hip_L_Joint_vel"     joint="hip_L_Joint" noise="0.01"/>
        <jointvel name="knee_L_Joint_vel"    joint="knee_L_Joint" noise="0.01"/>
        <jointvel name="ankle_L_Joint_vel"   joint="ankle_L_Joint" noise="0.01"/>

        <jointactuatorfrc name="abad_R_Joint_torque"    joint="abad_R_Joint" noise="0.01"/>
        <jointactuatorfrc name="hip_R_Joint_torque"     joint="hip_R_Joint" noise="0.01"/>
        <jointactuatorfrc name="knee_R_Joint_torque"    joint="knee_R_Joint" noise="0.01"/>
        <jointactuatorfrc name="ankle_R_Joint_torque"   joint="ankle_R_Joint" noise="0.01"/>
        <jointactuatorfrc name="abad_L_Joint_torque"    joint="abad_L_Joint" noise="0.01"/>
        <jointactuatorfrc name="hip_L_Joint_torque"     joint="hip_L_Joint" noise="0.01"/>
        <jointactuatorfrc name="knee_L_Joint_torque"    joint="knee_L_Joint" noise="0.01"/>
        <jointactuatorfrc name="ankle_L_Joint_torque"   joint="ankle_L_Joint" noise="0.01"/>

        <framequat name="imu_quat" objtype="site" objname="imu" noise="0.01"/>
        <gyro name="imu_gyro" site="imu" noise="0.01"/>
        <accelerometer name="imu_acc" site="imu" noise="0.01"/>

        <framepos name="frame_pos" objtype="site" objname="imu" />
        <framelinvel name="frame_vel" objtype="site" objname="imu" />

        <touch name="foot_force_heel_R" site="heel_R_site" />
        <touch name="foot_force_toe_R" site="toe_R_site" />
        <touch name="foot_force_heel_L" site="heel_L_site" />
        <touch name="foot_force_toe_L" site="toe_L_site" />
    </sensor>
</mujoco>
